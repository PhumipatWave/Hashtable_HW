<Window x:Class="Hashtable_HW.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hashtable_HW"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="Add" Content="Add" HorizontalAlignment="Left" Margin="192,225,0,0" VerticalAlignment="Top" Width="75" Click="Add_Click"/>
        <Button x:Name="Show" Content="Show All" HorizontalAlignment="Left" Margin="356,225,0,0" VerticalAlignment="Top" Width="75" Click="Show_Click"/>
        <Button x:Name="Remove" Content="Remove" HorizontalAlignment="Left" Margin="548,225,0,0" VerticalAlignment="Top" Width="75" Click="Remove_Click"/>
        <TextBox x:Name="Value" HorizontalAlignment="Left" Height="23" Margin="192,168,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="431" Text="Value"/>
        <TextBox x:Name="Key" HorizontalAlignment="Left" Height="23" Margin="192,110,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="431" Text="Key"/>

    </Grid>
</Window>
